{
  "compileOnSave": true,
  "compilerOptions": {
    "rootDir": "src",
    "target": "es6",
    "declaration": false,
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "lib": ["dom", "es6"],
    "outDir": "static",
    "moduleResolution": "node"
  }
}